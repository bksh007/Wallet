<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.qpaysolutions.mywallet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyWallet">
        <activity android:name=".DashBoardActivity"></activity>
        <activity android:name=".ProfileFormActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".PhoneVerificationActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".SignInActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>